@model IEnumerable<Payroll_Mvc.salaryreport>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(Model => Model.id)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.User_id)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Age)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Month)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Salary)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.PfAmount)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Esi)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.ProffessionalTax)
        </th>
        <th>
            @Html.DisplayNameFor(Model => Model.Total)
        </th>
        <th></th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PfAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Esi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProffessionalTax)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>


        </tr>

    }

</table>
<div>
    <a href="~/Login/Redirect/" class="btn btn-default" style=" background-color:cornflowerblue">Back</a>
</div>
