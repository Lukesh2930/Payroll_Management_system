                @Html.DisplayFor(modelItem => item.Enddate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numoffdays)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reason)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Applydate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Leavedate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Approved)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.User_Name)
            </td>
           

        </tr>
    }
    <a href="~/Login/Redirect/" " class="btn btn-default" style=" background-color:cornflowerblue">Back</a>
